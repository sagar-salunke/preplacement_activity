//time complecity=O(n)
import java.util.*;
class Linersearch
{
public static void main (String args[])
{

Scanner sc=new Scanner(System.in);
System.out.println("how many number you want in your array");
int n=sc.nextInt();
int a[]=new int[n];
System.out.println("enter element");
for(int i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
System.out.println("enter data you want to search");
int data=sc.nextInt();
int flag=0;
for(int i=0;i<n;i++)
{
if(a[i]==data)
{
System.out.println("data present "+i+" location");
flag=1;
break;
}
}
if(flag==0)
System.out.println("data is absent");
}
}
